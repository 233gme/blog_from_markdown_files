{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/jsx-indent": [
      2,
      4
    ],
    "indent": [
      2,
      4
    ],
    "@typescript-eslint/strict-boolean-expressions": 0,
    "react/react-in-jsx-scope": 0,
    "jsx-quotes": [
      2,
      "prefer-double"
    ],
    "no-console": 2,
    "object-curly-spacing": [
      2,
      "always"
    ]
  }
}
